{
	"id": "nodebb-plugin-emailer-local-yandex",
	"url": "https://github.com/MegaGM/nodebb-plugin-emailer-local-yandex",
	"name": "Yandex Emailer",
	"description": "Allows NodeBB to send emails via Yandex SMTP",
	"library": "./index.js",
	"templates": "./templates",
	"hooks": [
		{ "hook": "action:email.send", "method": "send" },
		{ "hook": "filter:admin.header.build", "method": "admin.menu" },
		{ "hook": "static:app.load", "method": "init"}
	]
}
